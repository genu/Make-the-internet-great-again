{
  "name": "Make The Internet Great Again",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Any references to Donald Trump are replaced with inspiring quotes from Charles Spurgeon",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",      
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/lodash/dist/lodash.js",
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": ["quotes/*.json"],
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Make The Internet Great Again",
    "default_popup": "popup.html"
  }
}
